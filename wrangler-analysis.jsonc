{
  "name": "querido-diario-analysis-worker",
  "account_id": "dd6e3021bfaef6eec1a68e26a593f4ca",
  "main": "src/analysis-worker.ts",
  "compatibility_date": "2024-10-01",
  "compatibility_flags": ["nodejs_compat"],

  // Queue configuration
  "queues": {
    "consumers": [
      {
        "queue": "querido-diario-analysis-queue",
        "max_batch_size": 1,
        "max_batch_timeout": 30,
        "max_retries": 3,
        "dead_letter_queue": "querido-diario-analysis-dlq"
      }
    ],
    "producers": [
      {
        "binding": "WEBHOOK_QUEUE",
        "queue": "querido-diario-webhook-queue"
      }
    ]
  },

  // KV namespaces
  "kv_namespaces": [
    {
      "binding": "ANALYSIS_RESULTS",
      "id": "107e82df3dc944d7961dc0067ae0b09e"
    },
    {
      "binding": "OCR_RESULTS",
      "id": "c4ef3be0866640e58ee549af2295711b"
    },
    {
      "binding": "WEBHOOK_SUBSCRIPTIONS",
      "id": "e92d5d945523420ba0fa684fbb2f7347"
    }
  ],

  // Environment variables
  "vars": {
    "ENVIRONMENT": "production"
  }

  // Secrets (set via wrangler secret put)
  // - OPENAI_API_KEY: OpenAI API key for AI analysis
}
